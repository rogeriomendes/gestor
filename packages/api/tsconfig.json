{
  "extends": "@gestor/config/tsconfig.base.json",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "dist",
    "composite": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "react": ["./node_modules/react"],
      "react/*": ["./node_modules/react/*"]
    }
  }
}
